@model EtAndHkIde.Models.ImageCollection

<div class="card-group">
    @for (var i = 0; i < Model.Images.Length; i++)
    {
        <div class="card">
            <a href="~/images/@Model.Images[i].Path">
                <img class="card-img-top" src="~/images/@Model.Images[i].Path" />
            </a>
        </div>
    }
</div>


